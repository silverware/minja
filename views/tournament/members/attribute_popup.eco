<%= @formFor {}, (form) => %>
  <%= form.textField "Name", "name" %>
  <div class="form-group">
    <label class="col-sm-2 control-label" for="inputtyp"><%= @i18n.members.type %></label>
    <div class="col-sm-10">
      <select class="form-control" id="inputtyp">
        <option value="textfield"><%= @i18n.members.textfield %></option>
        <option value="checkbox">Checkbox</option>
      </select>
    </div>
  </div>
<% end %>
